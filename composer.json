{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "blade-ui-kit/blade-ui-kit": "^0.4",
        "codeat3/blade-phosphor-icons": "^1.4",
        "diglactic/laravel-breadcrumbs": "^8.0",
        "enlightn/enlightn": "^2.1",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^3.2",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^9.4",
        "laravel/tinker": "^2.7",
        "laravolt/avatar": "^5.0",
        "livewire/livewire": "^3.0@beta",
        "meilisearch/meilisearch-php": "^0.24.2",
        "spatie/laravel-responsecache": "^7.4",
        "spatie/laravel-sitemap": "^6.2",
        "spatie/laravel-tags": "^4.3",
        "spatie/url": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.12",
        "brianium/paratest": "^7.2",
        "codeat3/phpcs-styles": "^1.2",
        "deployer/deployer": "^7.1",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.23",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.6",
        "nunomaduro/phpinsights": "^2.5",
        "pestphp/pest": "^2.8",
        "phpunit/phpunit": "^10.0",
        "rector/rector": "^0.17",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Domain\\": "domain/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/url-helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan livewire:publish --assets"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "format": "vendor/bin/php-cs-fixer fix",
        "test": "@php artisan test --parallel"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
